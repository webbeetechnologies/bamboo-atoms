{
  "name": "bamboo-shoots",
  "version": "0.0.1",
  "main": "lib/commonjs/index.js",
  "react-native": "src/index.ts",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "repository": "git@github.com:webbeetechnologies/bamboo-shoots.git",
  "author": "Thet Aung <thetaung.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prepare:husky": "husky install",
    "prepare": "bob build",
    "run:example": "cd example && yarn start",
    "bootstrap": "yarn --cwd example && yarn"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "2.0.0",
    "@types/lodash.isnil": "^4.0.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/react": "^18.0.17",
    "@types/react-native": "^0.69.5",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.69.4",
    "react-native-builder-bob": "^0.18.3",
    "typescript": "^4.6.1-rc"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-native": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "dependencies": {
    "lodash.isnil": "^4.0.0",
    "lodash.merge": "^4.6.2"
  }
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
